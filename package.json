{
  "name": "ohcoe-hugo",
  "version": "0.0.0",
  "description": "Hugo site Oral Health Care in the Opiod Epidemic",
  "main": "index.js",
  "scripts": {
    "test": "mocha themes/ohcoe/static/js/tests",
    "watch-scss": "node-sass --output themes/ohcoe/static/css themes/ohcoe/static/scss/main.scss && postcss -u autoprefixer -r themes/ohcoe/static/css/main.css",
    "build-scss": "nodemon -e scss -x 'npm run watch-scss'",
    "eslint": "eslint themes/ohcoe/static/js/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ccnmtl/ohcoe-hugo.git"
  },
  "author": "Center for Teaching and Learning, Columbia University",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/ccnmtl/ohcoe-hugo/issues"
  },
  "devDependencies": {
    "autoprefixer": "^9.0.0",
    "bootstrap": "~4.1.3",
    "eslint": "~5.16.0",
    "eslint-config-scanjs": "^1.0.0-beta4",
    "eslint-plugin-no-unsafe-innerhtml": "^1.0.16",
    "eslint-plugin-security": "^1.4.0",
    "jquery": "^3.3.1",
    "jsdom": "~14.1.0",
    "mocha": "~6.1.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.12.5",
    "postcss-cli": "^6.0.0"
  },
  "homepage": "https://github.com/ccnmtl/ohcoe-hugo#readme",
  "dependencies": {}
